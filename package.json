{
  "name": "dota-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "serve": "nodemon -L --exec node --loader ts-node/esm ./src/server.ts",
    "watch": "tsc --watch",
    "dev": "concurrently \"npm run serve\" \"npm run watch\"",
    "generate": "drizzle-kit generate:pg",
    "push": "tsx ./src/db/migrate.ts",
    "drop": "drizzle-kit drop",
    "upsert": "tsx src/db/operations/upsert_all.ts",
    "delete": "tsx src/db/operations/delete_all.ts",
    "check": "tsx src/db/operations/check_rows_sequences.ts"
  },
  "author": "jaredzg",
  "license": "ISC",
  "dependencies": {
    "fastify": "^4.25.2",
    "drizzle-orm": "^0.29.0",
    "fs": "^0.0.1-security",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
